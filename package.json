{
  "name": "vite-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "build": "pnpm run server:build && pnpm run client:build && pnpm run storybook:build",
    "client:build": "vite build",
    "client:dev": "vite",
    "dev": "concurrently \"pnpm run server:dev\" \"pnpm run client:dev\" \"pnpm run storybook\"",
    "postinstall": "patch-package",
    "prepare": "husky install",
    "serve": "vite preview",
    "server:build": "tsc --project tsconfig.server.json",
    "server:dev": "ts-node-dev --project tsconfig.server.json src/server",
    "start": "node dist/server.js",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build -o dist/storybook",
    "test": "tsc --noEmit && prettier --check . && eslint . && stylelint \"**/*.css\""
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "framer-motion": "^10.12.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@storybook/addon-actions": "^7.2.2",
    "@storybook/addon-essentials": "^7.2.2",
    "@storybook/addon-links": "^7.2.2",
    "@storybook/cli": "^7.2.2",
    "@storybook/react": "^7.2.2",
    "@storybook/react-vite": "^7.2.2",
    "@storybook/testing-library": "^0.2.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.0.1",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.13",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "patch-package": "^7.0.0",
    "prettier": "2.8.8",
    "storybook": "^7.2.2",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.8",
    "webpack": ">=5"
  }
}
